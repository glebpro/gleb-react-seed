{
  "name": "gleb-react-seed",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">=8.1.0"
  },
  "description": "Starter boilerplate for React, using Webpack 2",
  "repository": {
    "type": "git",
    "url": "https://github.com/glebpro/gleb-react-seed.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack2",
    "boilerplate"
  ],
  "author": "Gleb Promokhov",
  "scripts": {
    "start": "echo 'Commands Available: npm run front/back/both/build/preview/test'",
    "front": "webpack-dev-server",
    "back": "node backend/index.js",
    "both": "webpack-dev-server & node backend/index.js",
    "build": "rm -rf ./build && NODE_ENV=\"production\" webpack",
    "preview": "rm -rf ./build && NODE_ENV=\"production\" webpack-dashboard -t 'Preview Mode - gleb-react-seed' -- webpack-dev-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.7.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.6.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "0.28.4",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.2",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "prepush": "^3.1.11",
    "redux-logger": "^3.0.6",
    "regenerator-runtime": "^0.10.5",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.7",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.2.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.16.0",
    "cors": "^2.8.4",
    "es6-promise": "^4.1.0",
    "express": "^4.15.3",
    "immutable": "^3.8.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-page-transition": "^3.0.2",
    "react-transition-group": "^1.2.0"
  }
